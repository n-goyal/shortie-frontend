{
  "name": "shortie-frontend",
  "version": "1.0.0",
  "description": "frontend for url shortner",
  "main": "index.js",
  "keywords": [
    "url",
    "shortner"
  ],
  "scripts": {
    "dev": "NODE_ENV=development parcel src/index.html --open",
    "test": "NODE_ENV=test echo \"Error: no test specified\" && exit 1",
    "build": "NODE_ENV=production parcel build dist/index.html --public-utl ./",
    "clear-build-cache": "rm -rf .cache/ dist/",
    "format": "prettier --write \"src/**/*.{js,jsx}\"",
    "lint": "eslint \"src/**/*.{js,jsx}\" --quiet"
  },
  "author": "Nitin Goyal",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.21.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-router": "^5.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.16",
    "@babel/preset-env": "^7.12.16",
    "@babel/preset-react": "^7.12.13",
    "babel-eslint": "^10.1.0",
    "cross-env": "^7.0.3",
    "eslint": "^7.20.0",
    "dotenv": "^8.2.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "parcel-bundler": "^1.12.4",
    "prettier": "^2.2.1"
  }
}
